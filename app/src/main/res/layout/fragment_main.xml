<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.MainFragment">

    <Button
        android:id="@+id/MainFragmentToLedPanelFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="124dp"
        android:text="MainFragmentToLedPanelFragment"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/mainFragmentToPlotPanelFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MainFragmentToPlotPanelFragment"
        app:layout_constraintEnd_toEndOf="@+id/MainFragmentToLedPanelFragment"
        app:layout_constraintTop_toBottomOf="@+id/MainFragmentToLedPanelFragment" />

    <Button
        android:id="@+id/mainFragmentToBBB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MainFragmentToBBB"
        app:layout_constraintEnd_toEndOf="@+id/mainFragmentToPlotPanelFragment"
        app:layout_constraintTop_toBottomOf="@+id/mainFragmentToPlotPanelFragment" />

    <Button
        android:id="@+id/setIP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginBottom="24dp"
        android:text="Ustaw"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/setIPText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:ems="10"
        android:hint="http://192.168.69.69"
        android:text="http://192.168."
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/setIP"
        app:layout_constraintEnd_toEndOf="@+id/setIP"
        app:layout_constraintStart_toStartOf="@+id/setIP" />


</androidx.constraintlayout.widget.ConstraintLayout>